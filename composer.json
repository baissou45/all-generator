{
    "name": "baiss/all-generator",
    "description": "Ce package vise à simplifier la création des Controllers, Models et des vues pour les différents modules de nos projets laravel",
    "license": "MIT",
    "keywords": ["laravel", "crud", "controller", "crud-controller", "laravel-crud-controller", "view", "view-generator", "laravel-view-generator"],
    "autoload": {
        "psr-4": {
            "Baiss\\ViewGenerator\\": "src/",
            "Baiss\\ViewGenerator\\Test\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Taïrou Baïssou",
            "email": "tbaissou@gmail.com",
            "homepage": "https://baissou.mega-devs.com",
            "role": "Developper Flutter | Laravel"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "doctrine/dbal": "^3.6"
    },
    "extra": {
        "laravel": {
            "provider": [
                "Baiss\\ViewGenerator\\ServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/baissou45/all-generator/issues"
    },
    "config":{
        "optimize-autoloader":true
    }
}
